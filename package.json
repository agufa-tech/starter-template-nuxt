{
  "private": false,
  "name": "nuxt-starter-template",
  "version": "1.0.0",
  "description": "a Nuxt starter template with needed libraries, mainly for static sites, need a little modification for SSR",
  "repository": {
    "type": "git",
    "url": "git://github.com/agufa-tech/starter-template-nuxt"
  },
  "author": "agufa.tech",
  "license": "MIT",
  "scripts": {
    "dev": "ENV=dev nuxt dev",
    "dev:build": "ENV=dev nuxt build",
    "stage:dev": "ENV=stage nuxt dev",
    "stage": "ENV=stage nuxt build",
    "build": "ENV=prod nuxt build",
    "generate": "ENV=prod nuxt generate",
    "preview": "ENV=prod nuxt preview",
    "start": "node .output/server/index.mjs",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "prepare": "husky install",
    "generate:locales": "node tools/translator.js ./locales en.yml"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/json": "^2.1.70",
    "@intlify/nuxt3": "^0.2.2",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/svg": "^0.4.0",
    "@nuxtjs/tailwindcss": "^5.1.3",
    "@tailwindcss/forms": "^0.5.2",
    "@vueuse/core": "^8.7.5",
    "@vueuse/nuxt": "^8.7.5",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.4",
    "prettier": "^2.7.1",
    "pug": "^3.0.2",
    "typescript": "^4.7.4",
    "unplugin-icons": "^0.14.6",
    "unplugin-vue-components": "^0.20.1"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.1.9",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate": "^1.6.1"
  }
}
